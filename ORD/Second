# Mark the first View Full Details as processed (already scraped above)
print("\nMarking first View Full Details as processed...")
try:
    vfd_buttons = driver.find_elements(By.CSS_SELECTOR, "button[data-v-652da20a='']")
    if vfd_buttons:
        driver.execute_script("arguments[0].setAttribute('data-processed','1');", vfd_buttons[0])
        print("✓ Marked first View Full Details as processed")
    else:
        print("⚠ No View Full Details buttons found to mark")
except Exception as e:
    print(f"⚠ Could not mark first View Full Details: {e}")

# Loop over remaining View Full Details buttons
print("\nProcessing remaining View Full Details buttons in a loop...")
main_handle = dataset_handle  # Use the saved dataset handle
processed_count = 1  # first entry already scraped
while True:
    try:
        vfd_btn = WebDriverWait(driver, 6).until(
            EC.presence_of_element_located((By.CSS_SELECTOR, "button[data-v-652da20a='']:not([data-processed])"))
        )
    except Exception:
        print("No more View Full Details buttons found. Loop done.")
        break

    try:
        processed_count += 1
        print(f"\n{'='*60}")
        print(f"Processing View Full Details #{processed_count}...")
        print(f"{'='*60}")
        
        driver.execute_script("arguments[0].scrollIntoView({block: 'center'});", vfd_btn)
        time.sleep(0.4)
        # Mark as processed to avoid re-clicking the same button
        driver.execute_script("arguments[0].setAttribute('data-processed','1');", vfd_btn)

        target_url = None
        
        # Try to find parent link with href
        try:
            parent_link = vfd_btn.find_element(By.XPATH, "ancestor::a[@href]")
            target_url = parent_link.get_attribute('href')
            print(f"✓ Found target URL from parent link: {target_url}")
        except Exception as e1:
            print(f"⚠ Could not find parent link: {e1}")
            
            # Fallback: Try to extract href from parent's onclick or data attributes
            try:
                onclick = vfd_btn.get_attribute('onclick')
                if onclick:
                    print(f"onclick attribute: {onclick}")
                # Try to find router link or similar
                parent = vfd_btn.find_element(By.XPATH, "ancestor::*[@href or @to or @data-href]")
                target_url = parent.get_attribute('href') or parent.get_attribute('to') or parent.get_attribute('data-href')
                print(f"✓ Found target URL from parent attribute: {target_url}")
            except Exception as e2:
                print(f"⚠ Could not find from attributes: {e2}")

        if target_url:
            driver.execute_script("window.open(arguments[0], '_blank');", target_url)
            print(f"✓ Opened View Full Details in new tab: {target_url}")
            time.sleep(2)
        else:
            print("⚠ No target URL found; cannot proceed to avoid browse page navigation")
            continue

        # Switch to new tab
        current_handles = set(driver.window_handles)
        new_handle = None
        for h in current_handles:
            if h != main_handle:
                new_handle = h
        if not new_handle:
            print("⚠ No new tab handle found; skipping this entry")
            continue
        driver.switch_to.window(new_handle)
        print("✓ Switched to details tab")
        wait_ready(10)
        time.sleep(2)

        # Scroll down to find the code button for Base tab
        print("\nScrolling to find code button...")
        try:
            for _ in range(10):
                driver.execute_script("window.scrollBy(0, 75);")
                time.sleep(0.3)
            
            code_button = WebDriverWait(driver, 10).until(
                EC.element_to_be_clickable((By.CSS_SELECTOR, "div[data-v-144eb116].button"))
            )
            driver.execute_script("arguments[0].scrollIntoView({block: 'center'});", code_button)
            time.sleep(0.5)
            code_button.click()
            print("✓ Clicked code button (<>) for Base")
            time.sleep(2)
        except Exception as e:
            print(f"⚠ Failed to find/click code button: {e}")

        # Scrape Base data
        print("\nScraping Base data...")
        try:
            time.sleep(1)
            code_elements = driver.find_elements(By.CSS_SELECTOR, "pre, code")
            base_value = None
            base_reaction_role = None
            for element in code_elements:
                try:
                    text = element.text
                    if '"value":' in text and not base_value:
                        value_match = re.search(r'"value":\s*"([^"]+)"', text)
                        if value_match:
                            base_value = value_match.group(1)
                            print(f"✓ Found Base value: {base_value}")
                    if 'reaction_role' in text and not base_reaction_role:
